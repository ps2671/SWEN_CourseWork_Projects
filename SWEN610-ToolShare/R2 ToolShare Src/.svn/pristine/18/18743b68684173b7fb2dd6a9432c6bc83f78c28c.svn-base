{% extends "nav_bar.html" %}

{% block main_panel_template %}

<!-- Change Password View - Start -->

<div id="profile" class="container-fluid">
    <div class="row">
    <div class="col-sm-10"> 
    <div class="panel panel-primary" style="border-color: #34DDDD;">
        <div class="panel-heading" style="background-color: #34DDDD;">
            <div class="center-align" style="color: #000000;">
                <b>Change Password</b>
            </div>
        </div>
        <div class="panel-body">
<form method="post" action="/changePassword/">{% csrf_token %}
    <table class="center-align-table" border="0">

            {{ form.as_table }}
        <td><span style="color:#ff0000">{{form.as_table.errors}}</span></td>
        </table>
    <div class="center-align">
       <!-- <button type="submit" value="Change Password" name="change_Password"><button/>-->
            <button type="submit"  class="btn btn-primary" name="change_Password">Change Password</button>
    </div>
</form>
</div>
</div>
</div>
</div>
</div>
<!-- Change Password View - End -->
</div>
</div>
{% endblock %}
